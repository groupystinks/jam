{
  "name": "Jam",
  "description": "JAM",
  "author": "Lai Chia Sheng",
  "license": "MIT",
  "version": "0.0.1",
  "repository": {
    "type": "git",
    "url": ""
  },
  "keywords": [
    "react"
  ],
  "main": "babel.server.js",
  "scripts": {
    "clean": "rimraf dist",
    "start": "node ./node_modules/better-npm-run start",
    "build": "webpack --verbose --colors --display-error-details --config webpack/prod.config.js",
    "lint": "eslint -c .eslintrc src",
    "start-dev": "node ./node_modules/better-npm-run start-dev",
    "watch-client": "node ./node_modules/better-npm-run watch-client",
    "dev": "concurrent --kill-others \"npm run watch-client\" \"npm run start-dev\""
  },
  "betterScripts": {
    "start": {
      "command": "node ./babel.server.js",
      "env": {
        "NODE_PATH": "./src",
        "NODE_ENV": "production",
        "PORT": 8080
      }
    },
    "start-dev": {
      "command": "node ./babel.server.js",
      "env": {
        "NODE_PATH": "./src",
        "NODE_ENV": "development"
      }
    },
    "watch-client": {
      "command": "node webpack/webpack-dev-server.js",
      "env": {
        "UV_THREADPOOL_SIZE": 100,
        "NODE_PATH": "./src"
      }
    }
  },
  "dependencies": {
    "babel": "^5.4.7",
    "babel-plugin-typecheck": "0.0.3",
    "body-parser": "^1.13.2",
    "compression": "^1.5.0",
    "express": "^4.13.0",
    "express-session": "^1.11.3",
    "file-loader": "^0.8.4",
    "foundation-sites": "5.5.2",
    "http-proxy": "^1.11.1",
    "immutable": "^3.7.4",
    "nexusui": "^1.0.8",
    "piping": "0.1.8",
    "pretty-error": "^1.1.2",
    "radium": "^0.13.8",
    "react": "0.13.3",
    "react-redux": "1.0.1",
    "react-router": "v1.0.0-beta3",
    "redux": "1.0.1",
    "redux-devtools": "1.1.2",
    "redux-form": "^0.0.5",
    "serialize-javascript": "^1.0.0",
    "serve-favicon": "^2.3.0",
    "serve-static": "^1.10.0",
    "superagent": "^1.2.0",
    "tinytinycolor": "0.0.3",
    "tone": "^0.5.1",
    "url-loader": "^0.5.6"
  },
  "devDependencies": {
    "autoprefixer-loader": "^2.0.0",
    "babel-core": "5.4.7",
    "babel-eslint": "^3.1.18",
    "babel-loader": "5.1.3",
    "babel-runtime": "5.4.7",
    "better-npm-run": "0.0.1",
    "clean-webpack-plugin": "^0.1.3",
    "concurrently": "0.1.1",
    "eslint": "1.2.1",
    "eslint-config-airbnb": "0.0.8",
    "eslint-loader": "1.0.0",
    "eslint-plugin-react": "3.3.0",
    "extract-text-webpack-plugin": "^0.8.1",
    "json-loader": "0.5.2",
    "node-sass": "^3.2.0",
    "nodemon": "^1.4.1",
    "react-a11y": "0.1.1",
    "react-hot-loader": "1.2.7",
    "rimraf": "2.4.3",
    "sass-loader": "^1.0.2",
    "strip-loader": "^0.1.0",
    "style-loader": "^0.12.3",
    "webpack": "^1.9.11",
    "webpack-dev-server": "1.9.0"
  },
  "engines": {
    "node": ">=0.10.32"
  }
}
